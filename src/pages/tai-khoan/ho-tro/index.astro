---
import Layout from "../../../layouts/Layout.astro";
import HeaderMain from "../../../components/Header/Header";
import UserAccountLayout from "../../../components/UserPage/UserPageLayout";
import CustomerChat from "../../../components/UserPage/CustomerChat";
const pageTitle = `Địa chỉ giao hàng`;
const breadcrumbTitle = `Địa chỉ giao hàng`;
const siteTitle = `MTSMART - Cửa hàng điện thoại, laptop giá rẻ, chính hãng`;
import Footer from "../../../components/Footer/Footer";
---

<Layout>
    <Fragment slot="head">
        <title>{pageTitle} | {siteTitle}</title>
    </Fragment>

    <HeaderMain client:only />

    <main>
        <UserAccountLayout breadcrumbTitle={breadcrumbTitle} client:only>
            <CustomerChat client:only />
        </UserAccountLayout>
    </main>
    <Footer />
</Layout>
